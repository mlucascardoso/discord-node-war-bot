{
    "name": "discord-node-war-bot",
    "version": "1.0.0",
    "main": "index.js",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "start": "node backend/src/server.js",
        "build": "cd frontend && yarn install && yarn build",
        "backend:start": "node backend/src/server.js",
        "backend:dev": "nodemon backend/src/server.js",
        "backend:discord": "node backend/src/discord.js",
        "frontend:start": "cd frontend && yarn dev",
        "frontend:build": "cd frontend && yarn build",
        "frontend:install": "cd frontend && yarn install",
        "dev": "concurrently \"yarn backend:dev\" \"yarn frontend:start\"",
        "install:all": "yarn install && yarn frontend:install"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@vitejs/plugin-react": "^4.3.1",
        "concurrently": "^9.2.1",
        "eslint": "^9.9.0",
        "eslint-plugin-prettier": "^5.5.1",
        "nodemon": "^3.0.2",
        "prettier": "^3.6.2",
        "vite": "^5.4.8"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "discord.js": "^14.22.1",
        "dotenv": "^16.4.5",
        "express": "^5.1.0",
        "multer": "^2.0.2",
        "pg": "^8.16.3",
        "tesseract.js": "^6.0.1"
    },
    "engines": {
        "node": ">=22.11.0"
    }
}
