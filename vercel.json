{
    "buildCommand": "cd frontend && yarn install && yarn build",
    "outputDirectory": "frontend/dist",
    "functions": {
        "backend/api/*.js": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/api/$1"
        }
    ]
}