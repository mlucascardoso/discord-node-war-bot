{
    "version": 2,
    "buildCommand": "VITE_API_URL='' yarn frontend:install && yarn frontend:build",
    "outputDirectory": "frontend/dist",
    "builds": [
        {
            "src": "backend/api/*.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/api/$1"
        }
    ]
}