{
    "version": 2,
    "buildCommand": "VITE_API_URL='' yarn frontend:install && yarn frontend:build",
    "outputDirectory": "frontend/dist",
    "functions": {
        "backend/api/*.js": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/api/$1"
        }
    ]
}