{
    "buildCommand": "cd frontend && yarn install && VITE_API_URL='' yarn build",
    "outputDirectory": "frontend/dist",
    "functions": {
        "backend/api/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/api/$1"
        }
    ]
}